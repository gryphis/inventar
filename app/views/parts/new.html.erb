<h2>New Part</h2>
<%= form_for(@part) do |f| %>
<p>
  <%= f.label :desc, "Description" %><br/>
  <%= f.text_field :desc %>
</p>
<p>
  <%= f.label :serial, "Serial" %><br/>
  <%= f.text_field :serial %>
</p>
<p>
  <%= f.label :price, "Price" %><br/>
  <%= f.number_field :price, :min => 0 %>
</p>
<p>
  <%= f.label :datevid, "DatevID" %><br/>
  <%= f.number_field :datevid, :min => 0 %>
</p>
<%= f.submit %>
<% end %>
