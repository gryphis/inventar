<h2> Parts <span class="pluslink"><%= link_to "New", new_part_path %></span></h2>

<div class="parts">
<table border=1>
  <tr>
    <th colspan=3 align=left>Description</th>
  </tr>
  <tr>
    <th>Serial</th>
    <th>Datev_ID</th>
    <th>Price</th>
  </tr>
  <% @parts.each do |p| @part=p %>
    <!-- %= link_to p.desc, p % -->
    <!-- %= render "list" % -->
    <tr>
      <td colspan=3><%= link_to @part.desc, @part %></td>
    </tr>
    <tr>
      <td><%= @part.serial %></td>
      <td><%= @part.datev_id %></td>
      <td><%= @part.price.to_s %></td>
    </tr>
  <% end %>
</table>
</div>
